<h1>Founder Profiles</h1>

<div class="center">
  <div class="row">
    <% @profiles.each do |profile| %>
      <div class="col-md-3">
        <div class="thumbnail">
          <%= link_to image_tag(profile.user.image.url), profile %>
          <div class="caption">
            <h3><%= profile.user.id %></h3>
            <p><%= profile.expertise %></p>
            <p><%= "Seeking: #{profile.seeking}" %></p>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>




<%= link_to 'New Profile', new_profile_path %>
