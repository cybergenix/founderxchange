<%= simple_form_for(@profile) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :expertise %>
    <%= f.input :seeking %>
    <%= f.input :partner_role %>
    <%= f.input :about %>
    <%= f.input :experience %>
    <%= f.input :prefer_age %>
    <%= f.input :equity_type %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
